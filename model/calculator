
const multiplication = function (multiplyingMinimum, multiplyingMaximum, multiplierMinimum ,multiplierMaximum){
   
    let minimumMultiplying  = multiplyingMinimum
    let  maximumMultiplying = multiplyingMaximum
    let minimumMultiplier = multiplierMinimum
    let maximumMultiplier = multiplierMaximum
    let counter

  
  while (minimumMultiplying  <=  maximumMultiplying) {
        counter = minimumMultiplier

        while (counter <= maximumMultiplier) {
            
            let resultado = minimumMultiplying  *counter
            //stars ☆ ☆ ☆ ☆ ☆ ☆ ☆ ☆ ☆ 
            console.log(`☆  ${minimumMultiplying } x ${counter} = ${resultado}  ☆`)
            counter++
        }
        minimumMultiplying ++
        //return resultado
    }           
  
}

module.exports = {

    multiplication
}